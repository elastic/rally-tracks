{
  "repoOwner": "elastic",
  "repoName": "rally-tracks",
  "targetBranchChoices": [
    "9.2",
    "9.1",
    "8.15",
    "8.14",
    "8.13"
  ],
  "targetPRLabels": [
    "backport"
  ],
  "branchLabelMapping": {
    "^v(\\d{1,2})$" : "$1",
    "^v(\\d{1,2}).(\\d{1,2})$" : "$1.$2"
  },
  "autoMerge": true,
  "autoMergeMethod": "squash",
  "prDescription": "{defaultPrDescription}\n\n<!--BACKPORT {commits} BACKPORT-->"
}

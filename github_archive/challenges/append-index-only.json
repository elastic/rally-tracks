{
  "name": "append-index-only",
  "description": "Index the document corpus",
  "schedule": [
    {%- if p_data_stream %}
    {{ rally.collect(parts="../schedules/setup_data_stream.json") }}
    {%- else %}
    {{ rally.collect(parts="../schedules/setup_standard_index.json") }}
    {% endif -%},
    {
      "operation": "index",
      "warmup-time-period": 240,
      "clients": {{bulk_indexing_clients | default(8)}}
    }
  ]
}

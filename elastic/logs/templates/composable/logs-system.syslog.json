{
  "name": "logs-system.syslog",
  "index_template": {
    "index_patterns": [
      "logs-system.syslog-*"
    ],
    "template": {
      "settings": {},
      "mappings": {
        "_meta": {
          "package": {
            "name": "system"
          },
          "managed_by": "fleet",
          "managed": true
        }
      }
    },
    "composed_of": [
      "logs@settings",
      "logs-system.syslog@package",
      "logs-system.syslog@custom",
      "ecs@mappings",
      ".fleet_globals-1",
      ".fleet_agent_id_verification-1"
    ],
    "priority": 200,
    "_meta": {
      "package": {
        "name": "system"
      },
      "managed_by": "fleet",
      "managed": true
    },
    "data_stream": {
      "hidden": false,
      "allow_custom_routing": false
    },
    "ignore_missing_component_templates": [
      "logs-system.syslog@custom"
    ]
  }
}
{
  "name": "large-none",
  "description": "Benchmarking _has_privileges API with large scale and no wildcards (index)",
  "default": false,
  "schedule": [
    {
      "operation": "create-roles-and-users",
      "num_roles": 1000,
      "num_users": 100,
      "num_roles_per_user": 300,
      "num_spaces": 100,
      "wildcard_mode": "none"
    },
    {
      "operation": "create-kibana-app-privileges"
    },
    {
      "name": "has-privileges",
      "operation": "has-privileges",
      "num_users": 100,
      "num_spaces": 100,
      "warmup-iterations": 1,
      "iterations": 10,
      "clients": 5
    }
  ]
}

{
  "id": "Add terms aggregation to Response codes over time [Logs Apache] - part c",
  "name": "POST /api/ui_counters/_report",
  "requests": [
    {
      "stream": [
        {
          "name": "sleep",
          "operation-type": "sleep",
          "duration": 0.308608
        },
        {
          "name": "Elasticsearch: POST /logs-*/_async_search - apache - 5c.1",
          "operation-type": "search",
          "index": "logs-*",

{
  "name": "large-suffix-8.x",
  "description": "Benchmarking _has_privileges API with large scale and suffix wildcards only (index*) - Kibana 8.x",
  "default": false,
  "schedule": [
    {
      "operation": "create-roles-and-users",
      "num_roles": 1000,
      "num_users": 100,
      "num_roles_per_user": 300,
      "num_spaces": 100,
      "wildcard_mode": "suffix"
    },
    {
      "operation": "create-kibana-app-privileges",
      "version": "8.19.7"
    },
    {
      "name": "has-privileges",
      "operation": "has-privileges",
      "num_users": 100,
      "num_spaces": 100,
      "warmup-iterations": 1,
      "iterations": 10,
      "clients": 5
    }
  ]
}

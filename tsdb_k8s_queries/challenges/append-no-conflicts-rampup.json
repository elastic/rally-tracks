    {
      "name": "append-no-conflicts-rampup",
      "description": "Indexes the whole document corpus. Ramps up the number of clients with each timed operation.",
      "default": false,
      "schedule": [
        {
          "name": "create-all-templates",
          "operation": {
            "operation-type": "create-composable-template",
            "request-params": {
              "create": "true"
            }
          }
        },
        {
          "name": "put-timestamp-pipeline",
          "operation": {
            "operation-type": "put-pipeline",
            "id": "timestamp_pipeline",
            "body": {
              "processors": [
                  {
                      "set": {
                          "field": "@timestamp",
                          "value": {{'"{{_ingest.timestamp}}"'}}
                      }
                  },
                  {
                    "script": {
                      "source": "Random r  = new Random();ctx['kubernetes']['pod']['uid'] = 'value-' + r.nextLong();"
                    }
                  }
              ]
            }
          }
        },
        {
          "name": "check-cluster-health",
          "operation": {
            "operation-type": "cluster-health",
            "request-params": {
              "wait_for_status": "{{cluster_health | default('green')}}",
              "wait_for_no_relocating_shards": "true"
            },
            "retry-until-success": true
          }
        },
        {
          "operation": "index-container",
          "warmup-time-period": 240,
          "time-period": 900,
          "clients": {{bulk_indexing_clients | default(8)}}
        },
        {
          "operation": "index-pod",
          "warmup-time-period": 240,
          "clients": {{bulk_indexing_clients | default(8)}}
        }
        {# serverless-post-ingest-sleep-marker-start #}{%- if post_ingest_sleep|default(false) -%},
        {
          "name": "post-ingest-sleep",
          "operation": {
            "operation-type": "sleep",
            "duration": {{ post_ingest_sleep_duration|default(30) }}
          }
        },
        {%- endif -%}{# serverless-post-ingest-sleep-marker-end #}
        {%- set p_task_count = task_count | default(6) %}
        {%- for step in range(1, p_task_count + 1, 1) %},
              {%- set task_label = "ramp-up-" %}
              {%- set touch_bulk_indexing_clients = touch_bulk_indexing_clients * step %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_cpu_usage_per_pod_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-pod-1-{{name}}",
                "operation": "touch-pod-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_cpu_usage_per_pod_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_memory_usage_per_pod_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-pod-2-{{name}}",
                "operation": "touch-pod-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_memory_usage_per_pod_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_status_per_pod_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-pod-3-{{name}}",
                "operation": "touch-pod-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_status_per_pod_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_tx_network_usage_per_pod_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-pod-4-{{name}}",
                "operation": "touch-pod-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_tx_network_usage_per_pod_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_average_container_cpu_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-container-1-{{name}}",
                "operation": "touch-container-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_average_container_cpu_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_average_container_memory_usage_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-container-2-{{name}}",
                "operation": "touch-container-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_average_container_memory_usage_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_cpu_usage_per_container_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-container-3-{{name}}",
                "operation": "touch-container-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_cpu_usage_per_container_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_memory_usage_per_container_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-container-4-{{name}}",
                "operation": "{{task_label}}{{step}}_touch-container-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_memory_usage_per_container_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_unique_deployment_count_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-pod-5-{{name}}",
                "operation": "touch-pod-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_unique_deployment_count_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        },
              {%- endfor %}
              {%- for name, info in time_intervals.items() %}
        {
          "parallel": {
            "completed-by": "{{task_label}}{{step}}_percentile_cpu_usage_per_container_{{name}}",
            "tasks": [
              {
                "name": "{{task_label}}{{step}}_touch-container-5-{{name}}",
                "operation": "touch-container-index",
                "clients": {{touch_bulk_indexing_clients}}
              },
              {
                "operation": "{{task_label}}{{step}}_percentile_cpu_usage_per_container_{{name}}",
                "warmup-iterations": {{search_warmup_iterations}},
                "iterations": {{search_iterations}},
                "target-interval": {{target_interval}},
                "clients": 1
              }
            ]
          }
        }{{ ", " if not loop.last else "" }}
        {%- endfor %}
        {%- endfor %}
      ]
    }

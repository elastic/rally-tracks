{
  "name": ".logs-endpoint.actions",
  "index_template": {
    "index_patterns": [
      ".logs-endpoint.actions-*"
    ],
    "template": {
      "settings": {
        "index": {
          "default_pipeline": "logs-endpoint.actions-8.2.0"
        }
      },
      "mappings": {
        "_meta": {
          "package": {
            "name": "endpoint"
          },
          "managed_by": "fleet",
          "managed": true
        }
      }
    },
    "composed_of": [
      ".logs-endpoint.actions@package",
      ".logs-endpoint.actions@custom",
      ".fleet_globals-1",
      ".fleet_agent_id_verification-1"
    ],
    "priority": 200,
    "_meta": {
      "package": {
        "name": "endpoint"
      },
      "managed_by": "fleet",
      "managed": true
    },
    "data_stream": {
      "hidden": true,
      "allow_custom_routing": false
    }
  }
}
{
  "name": "append-no-conflicts-rampup-stream",
  "description": "Indexes the whole document corpus. Ramps up the number of clients with each timed operation.",
  "default": false,
  "schedule": [
    {
      "name": "create-all-templates",
      "operation": {
        "operation-type": "create-composable-template",
        "request-params": {
          "create": "true"
        }
      }
    },
    {
      "name": "put-timestamp-pipeline",
      "operation": {
        "operation-type": "put-pipeline",
        "id": "timestamp_pipeline",
        "body": {
          "processors": [
              {
                  "set": {
                      "field": "@timestamp",
                      "value": {{'"{{_ingest.timestamp}}"'}}
                  }
              },
              {
                "script": {
                  "source": "Random r  = new Random();ctx['kubernetes']['pod']['uid'] = 'value-' + r.nextLong();"
                }
              }
          ]
        }
      }
    },
    {
      "name": "check-cluster-health",
      "operation": {
        "operation-type": "cluster-health",
        "request-params": {
          "wait_for_status": "{{cluster_health | default('green')}}",
          "wait_for_no_relocating_shards": "true"
        },
        "retry-until-success": true
      }
    },
    {
      "parallel": {
        "tasks": [
          {%- set p_bulk_indexing_clients = bulk_indexing_clients | default(8) %}
          {%- set split_bulk_indexing_clients = p_bulk_indexing_clients / 2 %}
          {%- set split_bulk_indexing_clients = split_bulk_indexing_clients | int %}
          {
            "operation": "index-container",
            "time-period": 600,
            "clients": {{split_bulk_indexing_clients}}
          },
          {
            "operation": "index-pod",
            "time-period": 600,
            "clients": {{split_bulk_indexing_clients}}
          }
        ]
      }
    }
    {# serverless-post-ingest-sleep-marker-start #}{%- if post_ingest_sleep|default(false) -%}
    {
      "name": "post-ingest-sleep",
      "operation": {
        "operation-type": "sleep",
        "duration": {{ post_ingest_sleep_duration|default(30) }}
      }
    }
    {%- endif -%}{# serverless-post-ingest-sleep-marker-end #}
    {%- set p_iteration_count = iteration_count | default(6) %}
    {%- for step in range(1, p_iteration_count + 1, 1) %},
          {%- set touch_bulk_indexing_clients = touch_bulk_indexing_clients * step %}
          {%- set t_parallel_clients = touch_bulk_indexing_clients + 5 %}
          {%- set split_touch_bulk_indexing_clients = touch_bulk_indexing_clients/2 %}
          {%- set split_touch_bulk_indexing_clients = split_touch_bulk_indexing_clients | int %}
          {%- set task_label = "ramp-up-" %}
          {%- for name, info in time_intervals.items() %}
    {
      "parallel": {
        "clients": {{t_parallel_clients}},
        "tasks": [
          {
            "name": "{{task_label}}{{split_touch_bulk_indexing_clients}}_touch-pod-{{name}}",
            "operation": "touch-pod-index",
            "time-period": 600,
            "clients": {{split_touch_bulk_indexing_clients}}
          },
          {
            "name": "{{task_label}}{{split_touch_bulk_indexing_clients}}_touch-container-{{name}}",
            "operation": "touch-container-index",
            "time-period": 600,
            "clients": {{split_touch_bulk_indexing_clients}}
          },
          {
            "name": "{{task_label}}{{split_touch_bulk_indexing_clients}}_cpu_usage_per_pod_{{name}}",
            "index": "k8s-*",
            "operation-type": "search",
            "detailed-results": {{detailed_results | default("false")}},
            "iterations": {{search_iterations}},
            "target-interval": {{target_interval}},
            "clients": 1,
            "body": {
              "aggs": {
                "0": {
                  "terms": {
                    "field": "kubernetes.pod.name",
                    "order": {
                      "2": "desc"
                    },
                    "size": 100
                  },
                  "aggs": {
                    "1": {
                      "date_histogram": {
                        "field": "@timestamp",
                        "fixed_interval": "{{info[0]}}",
                        "time_zone": "Europe/Athens",
                        "min_doc_count": 1
                      },
                      "aggs": {
                        "2": {
                          "avg": {
                            "field": "kubernetes.pod.cpu.usage.limit.pct"
                          }
                        }
                      }
                    },
                    "2": {
                      "avg": {
                        "field": "kubernetes.pod.cpu.usage.limit.pct"
                      }
                    }
                  }
                }
              },
              "size": 0,
              "fields": [
                {
                  "field": "@timestamp",
                  "format": "date_time"
                },
                {
                  "field": "event.ingested",
                  "format": "date_time"
                },
                {
                  "field": "process.cpu.start_time",
                  "format": "date_time"
                },
                {
                  "field": "system.process.cpu.start_time",
                  "format": "date_time"
                }
              ],
              "script_fields": {},
              "stored_fields": [
                "*"
              ],
              "runtime_mappings": {},
              "_source": {
                "excludes": []
              },
              "query": {
                "bool": {
                  "must": [],
                  "filter": [
                    {
                      "match_phrase": {
                        "data_stream.dataset": "kubernetes.pod"
                      }
                    },
                    {
                      "range": {
                        "@timestamp": {
                          "format": "strict_date_optional_time",
                          "gte": "{{info[1]}}",
                          "lte": "{{end_time}}"
                        }
                      }
                    }
                  ],
                  "should": [],
                  "must_not": []
                }
              }
            }
          },
